 light_flicker_updater__template:t="factory_lamp_a_red_flicker_template"

}

factory_lamp_a_red_flicker_template{
  _use:t="light_flicker_with_sparks"

  light_flicker__attack_time:p2 = 0.01, 0.15 // required time for transition from "off" to "on" position
  light_flicker__on_time:p2 = 0.1, 1.5 // duration of position "on"
  light_flicker__release_time:p2 = 0.75, 1.25 // required time for transition from "on" to "off" position
  light_flicker__off_time:p2 = 0.01, 6 // duration of position "off"

  light_flicker__on_brightness:p2 = 0.6, 1.0 // during of position "on". It's multiplyed by light__brightness
  light_flicker__off_brightness:p2 = 0.0, 0.2 // during of position "off". It's multiplyed by light__brightness

  light_flicker__noise_brightness_on:p2=0.5,1.0 // it's multiplyer for low/high noise amplitude when light "on"
  light_flicker__noise_brightness_off:p2=0.0,1.0 // it's multiplyer for low/high noise amplitude when light "off"
  light_flicker__noise_time:r=8.0 // frequency of noise per sec
}

