demon_portal_flicker_char_template_15m_a{
  _use:t="light_flicker"

  light_flicker__attack_time:p2 = 0, 0 // required time for transition from "off" to "on" position
  light_flicker__on_time:p2 = 60, 180 // duration of position "on"
  light_flicker__release_time:p2 = 0, 0 // required time for transition from "on" to "off" position
  light_flicker__off_time:p2 = 0, 0 // duration of position "off"

  light_flicker__on_brightness:p2 = 1, 1 // during of position "on". It's multiplyed by light__brightness
  light_flicker__off_brightness:p2 = 0.0, 0.0 // during of position "off". It's multiplyed by light__brightness

  light_flicker__noise_brightness_on:p2=0.2,0.5 // it's multiplyer for low/high noise amplitude when light "on"
  light_flicker__noise_brightness_off:p2=0.2,0.5 // it's multiplyer for low/high noise amplitude when light "off"
  light_flicker__noise_time:r=3.0 // frequency of noise per sec
}

